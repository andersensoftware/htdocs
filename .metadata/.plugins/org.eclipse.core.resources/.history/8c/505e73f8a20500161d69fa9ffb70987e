<?php

$server = 'localhost';
$username = 'root';
$password = '';
$database = 'to_do_website';

// Create connection
$conn = new mysqli($server, $username, $password, $database);
// Check connection
if ($conn->connect_error) {
    die("Connection failed: " . $conn->connect_error);
}

$emailform = 'joa@jimmiandersen.dk';

$sql = 'SELECT `email` FROM `autorised` WHERE `email` = :emailform';
$result = $conn->query($sql);
echo $result;
echo strcmp($result,$emailform)

?>

